{
  "name": "@repo/server",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/app.ts",
  "scripts": {
    "dev": "tsx watch ./app.ts",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "prestart": "pnpm build",
    "preinstall": "npx only-allow pnpm",
    "start": "tsx ./app.ts",
    "test": "vitest .",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "mongoose": "^8.10.1",
    "multer": "1.4.5-lts.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "prisma-extension-random": "^0.2.2",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@faker-js/faker": "^9.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.5.1",
    "prisma": "^6.4.1",
    "supertest": "^7.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.18.0",
    "vitest": "^2.1.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ]
  }
}
